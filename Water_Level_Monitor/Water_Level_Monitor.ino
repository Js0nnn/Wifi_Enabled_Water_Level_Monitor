const unsigned long interval = 10 * 60 * 1000;  // 10 minutes in milliseconds
unsigned long previousMillis = 0;

bool buzzerEnabled = true;  // Flag to track buzzer state

// Function to display a bitmap on the OLED display
void displayBitmap(const unsigned char* bitmap, int width, int height) {
  display.clearDisplay();
  display.drawBitmap(0, 0, bitmap, width, height, WHITE);
  display.display();
}

// Function to beep the buzzer a specified number of times
void buzzerBeep(int beepCount) {
  for (int i = 0; i < beepCount; i++) {
    tone(buzzerPin, 1000); // Beep at 1000 Hz
    delay(500); // Beep duration
    noTone(buzzerPin);
    delay(500); // Delay between beeps
  }
}

float getDistance() {
  // Trigger the ultrasonic sensor
  digitalWrite(TRIG_PIN, LOW);
  delayMicroseconds(2);
  digitalWrite(TRIG_PIN, HIGH);
  delayMicroseconds(10);
  digitalWrite(TRIG_PIN, LOW);

  // Read the echo pulse duration
  float duration = pulseIn(ECHO_PIN, HIGH);

  // Calculate distance in centimeters
  float distance = duration * 0.034 / 2;

  return distance;
}

void setup() {
  pinMode(TRIG_PIN, OUTPUT);
  pinMode(ECHO_PIN, INPUT);
  pinMode(buzzerPin, OUTPUT);
  pinMode(buttonPin, INPUT_PULLUP);

  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  display.clearDisplay();
  delay(2000);
}

void loop() {
  unsigned long currentMillis = millis();

  if (currentMillis - previousMillis >= interval) {
    previousMillis = currentMillis;

    // Get average distance readings over 30 seconds
    unsigned long sumDistances = 0;
    int readingsCount = 0;
    unsigned long startTime = millis();

    while (millis() - startTime < 30000) {  // 30 seconds
      sumDistances += getDistance();
      readingsCount++;
      delay(100);  // Delay between readings
    }

    float averageDistance = sumDistances / readingsCount;

    // Determine the water level based on average distance
    int waterLevel = map(averageDistance, 0, 100, 100, 0);  // Adjust the mapping as needed

    // Select the appropriate bitmap based on the water level percentage
    if (waterLevel >= BMP_100_THRESHOLD) {
      displayBitmap(bmp_100, 128, 64);
    } else if (waterLevel >= BMP_90_THRESHOLD) {
      displayBitmap(bmp_90, 128, 64);
    } else if (waterLevel >= BMP_80_THRESHOLD) {
      displayBitmap(bmp_80, 128, 64);
    } else if (waterLevel >= BMP_70_THRESHOLD) {
      displayBitmap(bmp_70, 128, 64);
    } else if (waterLevel >= BMP_60_THRESHOLD) {
      displayBitmap(bmp_60, 128, 64);
    } else if (waterLevel >= BMP_50_THRESHOLD) {
      displayBitmap(bmp_50, 128, 64);
    } else if (waterLevel >= BMP_40_THRESHOLD) {
      displayBitmap(bmp_40, 128, 64);
    } else if (waterLevel >= BMP_30_THRESHOLD) {
      displayBitmap(bmp_30, 128, 64);
    } else if (waterLevel >= BMP_20_THRESHOLD) {
      displayBitmap(bmp_20, 128, 64);
    } else if (waterLevel >= BMP_10_THRESHOLD) {
      displayBitmap(bmp_10, 128, 64);
    } else if (currentMillis % interval < 10000) { // Blink and beep in the first 10 seconds of every 10 minutes
      for (int i = 0; i < 5; i++) {
        displayBitmap(bmp_0, 128, 64);
        buzzerBeep(1); // Beep once
        delay(500);
        display.clearDisplay();
        delay(500);
      }

      // Check if the push button is pressed to toggle the buzzer state
      if (digitalRead(buttonPin) == LOW) {
        buzzerEnabled = !buzzerEnabled; // Toggle the buzzer state
        delay(1000); // Debounce delay
      }

      delay(2000); // Delay for 2 seconds before checking again
    }
  }
}
